{
  "manifest_version": 3,
  "name": "BookMark Buddy",
  "version": "1.0",
  "description": "A Chrome extension to save and manage links.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}